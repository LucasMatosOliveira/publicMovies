{
  "cli": {
    "appVersionSource": "auto"
  },
  "build": {
    "preview": {
      "environment": "preview",
      "android": {
        "buildType": "apk"
      }
    },
    "preview2": {
      "environment": "preview",
      "android": {
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "preview3": {
      "environment": "preview",
      "developmentClient": true
    },
    "preview4": {
      "environment": "preview",
      "distribution": "internal"
    },
    "production": {
      "environment": "production"
    }
  }
}